{
  "name": "@jadetree/currency",
  "version": "0.8.0",
  "description": "Currency Localization for Jade Tree",
  "main": "dist/jt-currency.common.js",
  "module": "dist/jt-currency.esm.js",
  "browser": "dist/jt-currency.min.js",
  "repository": "http://vm-docker-5.lan.kraussnet.com:3000/jkrauss/jt-currency",
  "author": "Jonathan Krauss <jkrauss@asymworks.com>",
  "license": "BSD-2-Clause",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "docs:html": "documentation build src/**/*.ts -f html -o docs --parse-extension ts",
    "docs:md": "documentation build src/**/*.ts -f md --parse-extension ts",
    "lint": "eslint --ext .js,.ts src",
    "serve:docs": "documentation serve src/**/*.ts --parse-extension ts",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --require ts-node/register tests/**/*.ts --check-leaks --globals navigator,process"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "documentation": "^13.0.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^22.0.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
