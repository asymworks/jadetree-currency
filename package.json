{
  "name": "@jadetree/currency",
  "version": "0.8.17",
  "description": "Localized Currency Library for Jade Tree",
  "main": "./index.js",
  "module": "lib/index",
  "typings": "lib/index",
  "repository": "https://github.com/asymworks/jadetree-currency",
  "author": "Jonathan Krauss <jkrauss@asymworks.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "rm -rf index.js index.*.js index.*.map lib locales && node --experimental-json-modules build",
    "build-data": "npx yarn build-data:ccy && npx yarn build-data:l10n",
    "build-data:ccy": "node scripts/gen_ccy_data.js -o ./src/ccy-data.ts -d ./docs/data/currency-list.md",
    "build-data:l10n": "node scripts/gen_l10n_data.js -a ./src/locale-aliases.js -d ./docs/data/locale-list.md ./src/locales",
    "clean": "rm -rf index.js index.*.js index.*.map lib locales",
    "clean-data": "rm docs/data/currency-list.md docs/data/locale-list.md src/ccy-data.ts src/locales/*.ts",
    "docs:html": "documentation build src/*.ts -f html -o docs --parse-extension ts",
    "docs:md": "documentation build src/*.ts -f md --parse-extension ts",
    "docs:serve": "documentation serve --watch src/*.ts --parse-extension ts",
    "lint": "eslint --ext .js,.ts src",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --require ts-node/register tests/**/*.ts --check-leaks --globals navigator,process"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "argparse": "^2.0.1",
    "chai": "^4.2.0",
    "cldr-core": "^37.0.0",
    "cldr-numbers-modern": "^37.0.0",
    "cross-env": "^7.0.2",
    "documentation": "^13.2.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^22.0.0",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "node-html-parser": "^1.3.1",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-rewrite": "^0.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.1",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.0.3",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "decimal.js-light": "^2.5.1"
  }
}
